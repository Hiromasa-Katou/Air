            $(".my-heart-wrapper").click(function() {
                $svg = $(this).find("svg");
                $how = $svg.attr("fill-opacity");

                if ($how === "0") {
                    $(".my-heart").attr("fill-opacity", "1");
                    $(".my-heart").attr("fill", "#FF5A5F");
                    $(".my-heart").attr("stroke", "#FF5A5F");
                } else {
                    $(".my-heart").attr("fill-opacity", "0");
                    $(".my-heart").attr("fill", "#484848");
                    $(".my-heart").attr("stroke", "#484848");
                }

                $(this).preventDefault();
                return false;
            });

